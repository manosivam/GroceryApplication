package org.grocery.customers;
 

/**
 * @author Manosivam
 *
 */
public class SeniorCitizen extends Customer{

	/**
	 * @param name
	 * @throws CustomerException 
	 */
	public SeniorCitizen(String name, int age) throws CustomerException {
		super(name,age);
		this.type = CustomerType.SENIOR_CITIZEN;
	}
	
}

